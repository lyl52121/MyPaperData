<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.lyl.dao.SensorLocalMapper">
	<insert id="addSensorLocal" parameterType="SensorLocal">
		insert into sensor_local(id,sensorId,x,y)
		value(#{id},#{sensorId},#{x},#{y})
	</insert>
</mapper>	